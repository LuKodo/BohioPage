                                                            {name == "Todos" && <span className="material-icons fs-6 me-1">check_circle</span>}
                                                            <span className={name === filters.stratum ? "text-decoration-none text-white" : "text-decoration-none text-danger"} href="#">{name}</span>
                                                        </span>
                                                    )
                                                })}
                                            </div>
